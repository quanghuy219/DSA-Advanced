#include <stdio.h>
#include <stdlib.h>

typedef struct {
  int *matrix;
  int sizemax;
} Graph;

Graph createGraph(int sizemax) {
  Graph g;
  g.matrix = (int*)malloc(sizemax * sizemax * sizeof(int));
  g.sizemax = sizemax;
  return g;
}

void addEdge(Graph graph, int v1, int v2) {
  graph.matrix[v1 * sizeof(int) + v2] = 1;
  graph.matrix[v2 * sizeof(int) + v1] = 1;
}

int main()
{
  
  return 0;
}


